@model IEnumerable<Users>
<h2>Users</h2>

<table>
    <tr>
        <th>ID</th>
        <th>FirstName</th>
        <th>MiddleName</th>
        <th>LastName</th>
        <th>IDBalance</th>
        
    </tr>

    @foreach (var user in Model)
    {
        <tr>
            <td><a href=/User/ViewUser/@user.idUser>@user.idUser</a></td>
            <td>@user.FirstName</td>
            <td>@user.MiddleName</td>
            <td>@user.LastName</td>
            <td>@user.IDBalance</td>
            
        </tr>
    }

</table>